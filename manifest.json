{
    "name": "TimeIsMoney",
    "version": "1.0",
    "description": "Chrome extension that converts dollar amounts in webpage to time needed to make said dollar amount",
    "manifest_version": 2,
    "options_page": "options.html",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}