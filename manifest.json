{
    "name": "TimeIsMoney",
    "version": "1.0",
    "description": "Chrome extension that converts dollar amounts in webpage to time needed to make said dollar amount",
    "manifest_version": 2,
    "permissions": ["storage","activeTab"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [{
        "matches": ["*://*.stackoverflow.com/*"],
        "js": ["content.js"]
      }],
      "browser_action": {
        "default_title": "Test Extension"
      }
}